{
  "name": "@workspace/api",
  "exports": "./src/exports.ts",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn ./src/main.ts",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/src/main.js",
    "tsc:typecheck": "tsc --noEmit",
    "eslint:fix": "eslint . --fix",
